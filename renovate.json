{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "/netbird/"
      ],
      "matchManagers": [
        "rust"
      ],
      "matchUpdateTypes": [
        "patch",
        "lockFileMaintenance"
      ],
      "groupName": "Patch Dependencies",
      "enabled": true
    }
  ]
}
